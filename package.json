{
  "name": "user-api",
  "version": "1.0.0",
  "description": "Testing complete GCP CI/CD pipeline",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node test.js",
    "docker:build": "docker build -t user-api .",
    "docker:run": "docker run -p 3001:3001 user-api",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=user-api)",
    "deploy:staging": "bash ./deploy/cloud-run.sh staging",
    "deploy:production": "bash ./deploy/cloud-run.sh latest"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "nodejs",
    "express",
    "microservice",
    "gcp",
    "cloud-run"
  ],
  "author": "group:default/guests",
  "license": "ISC"
}
